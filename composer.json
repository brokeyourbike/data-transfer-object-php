{
    "name": "brokeyourbike/data-transfer-object",
    "description": "Return original response alongside with the DataTransferObject",
    "type": "library",
    "license": "MPL-2.0",
    "keywords": [
        "api",
        "data-transfer-object",
        "brokeyourbike"
    ],
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\DataTransferObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\DataTransferObject\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "spatie/data-transfer-object": "^3.7",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4",
        "phpunit/phpunit": "^9",
        "mockery/mockery": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "version": "0.2.2"
}
